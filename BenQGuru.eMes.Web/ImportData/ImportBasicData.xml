<?xml version="1.0" encoding="utf-8" ?>
<dataTypeList>
  <dataType name="WorkPlan" text="生产计划">
    <fieldList>
      <add key="PLANDATE" headerText="计划日期" Match="false" AllowNull="false"></add>
      <add key="BIGSSCODE" headerText="大线代码" Match="false" AllowNull="false"></add>
      <add key="PLANSEQ" headerText="生产顺序" Match="false" AllowNull="false" ></add>
      <add key="MOCODE" headerText="工单代码" Match="false" AllowNull="false"></add>
      <add key="MOSEQ" headerText="工单项次" Match="false" AllowNull="false"></add>
      <add key="PLANQTY" headerText="计划数量" Match="false" AllowNull="false"></add>
      <add key="PLANSTARTTIME" headerText="计划开始时间" Match="false" AllowNull="false"></add>
    </fieldList>
  </dataType>

  <dataType name="MaterialNeed" text="要料维护">
    <fieldList>
      <add key="ITEMCODE" headerText="物料代码" Match="false" AllowNull="false"></add>
      <add key="ORGID" headerText="组织代码" Match="false" AllowNull="false" ></add>
      <add key="REQUESTQTY" headerText="需求标准数量" Match="false" AllowNull="true" ></add>
    </fieldList>
  </dataType>

  <dataType name="PlanWorkTime" text="产品排产工时">
    <fieldList>
      <add key="ITEMCODE" headerText="产品代码" Match="false" AllowNull="true"></add>
      <add key="SSCODE" headerText="产线代码" Match="false" AllowNull="true"></add>
      <add key="CYCLETIME" headerText="CycleTime" Match="false" AllowNull="true"></add>
      <add key="WORKINGTIME" headerText="排产工时" Match="false" AllowNull="true"></add>
    </fieldList>
  </dataType>  
</dataTypeList>
