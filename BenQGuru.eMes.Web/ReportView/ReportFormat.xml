                        <TextAlign><%TextAlign%></TextAlign>
                        <VerticalAlign><%VerticalAlign%></VerticalAlign>
<%BorderStyle%>
]]>
  </TableCellStyle>
  <CellBorderStyle>
    <![CDATA[
                        <BorderStyle>
                          <Default><%BorderStyle%></Default>
                        </BorderStyle>
]]>
  </CellBorderStyle>
