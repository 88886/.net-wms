<![CDATA[
      <Chart Name="<%ChartName%>">
        <Type><%ChartType%></Type>
        <Legend>
          <Visible><%ShowLegend%></Visible>
          <Style>
            <BorderStyle>
              <Default>Solid</Default>
            </BorderStyle>
          </Style>
          <Position>RightCenter</Position>
        </Legend>
        <Subtype>Plain</Subtype>
        <Title />
        <Top><%Top%>cm</Top>
        <Height>8.75cm</Height>
        <CategoryAxis>
          <Axis>
            <Title />
            <MajorGridLines>
              <Style>
                <BorderStyle>
                  <Default>Solid</Default>
                </BorderStyle>
              </Style>
            </MajorGridLines>
            <MinorGridLines>
              <Style>
                <BorderStyle>
                  <Default>Solid</Default>
                </BorderStyle>
              </Style>
            </MinorGridLines>
            <MajorTickMarks>Outside</MajorTickMarks>
