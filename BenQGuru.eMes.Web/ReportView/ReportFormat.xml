        <Top><%Top%>cm</Top>
        <Height><%Height%>cm</Height>
        <Width><%Width%>cm</Width>
        <Style>
          <PaddingLeft>2pt</PaddingLeft>
          <PaddingBottom>2pt</PaddingBottom>
          <PaddingRight>2pt</PaddingRight>
          <PaddingTop>2pt</PaddingTop>
<%TableCellStyle%>
        </Style>
        <CanGrow>true</CanGrow>
        <Value><%TextValue%></Value>
      </Textbox>
]]>
  </ExtendText>
  <Table>
<![CDATA[
      <Table Name="table1">
        <DataSetName>MESRPT</DataSetName>
        <Top><%Top%>cm</Top>
        <Header>
          <TableRows>
            <TableRow>
              <TableCells>
<%HeaderCellList%>
              </TableCells>
              <Height>0.5cm</Height>
            </TableRow>
          </TableRows>
          <RepeatOnNewPage>true</RepeatOnNewPage>
        </Header>
<%TableGroups%>
        <Details>
          <TableRows>
            <TableRow>
              <TableCells>
<%TableCellList%>
              </TableCells>
              <Height>0.5cm</Height>
            </TableRow>
          </TableRows>
<%TableDetailVisibility%>
        </Details>
        <TableColumns>
<%TableColumnList%>
        </TableColumns>
      </Table>
]]>
  </Table>
  <TableGroups>
    <![CDATA[
        <TableGroups>
<%TableGroup%>
        </TableGroups>
]]>
  </TableGroups>
  <TableGroup>
    <![CDATA[
          <TableGroup>
            <Header>
              <TableRows>
                <TableRow>
                  <TableCells>
<%GroupCellList%>
                  </TableCells>
                  <Height>0.5cm</Height>
                </TableRow>
              </TableRows>
            </Header>
            <Sorting>
              <SortBy>
                <SortExpression><%GroupSortExpression%></SortExpression>
                <Direction>Ascending</Direction>
              </SortBy>
            </Sorting>
            <Grouping Name="<%GroupName%>">
              <GroupExpressions>
                <GroupExpression><%GroupExpression%></GroupExpression>
              </GroupExpressions>
            </Grouping>
<%Visibility%>
          </TableGroup>
]]>
  </TableGroup>
  <GroupCell>
    <![CDATA[
                <TableCell>
                  <ReportItems>
                    <Textbox Name="<%TextBoxName%>">
                      <rd:DefaultName><%TextBoxName%></rd:DefaultName>
                      <ZIndex>9</ZIndex>
                      <Style>
                        <PaddingLeft>2pt</PaddingLeft>
                        <PaddingBottom>2pt</PaddingBottom>
                        <BorderColor>
                          <Default>LightGrey</Default>
                        </BorderColor>
                        <PaddingRight>2pt</PaddingRight>
                        <PaddingTop>2pt</PaddingTop>
<%TableCellStyle%>
                      </Style>
                      <CanGrow>true</CanGrow>
                      <Value><%CellText%></Value>
                    </Textbox>
                  </ReportItems>
                </TableCell>
]]>
  </GroupCell>
  <TableCell>
    <![CDATA[
                <TableCell>
                  <ReportItems>
                    <Textbox Name="<%TextBoxName%>">
