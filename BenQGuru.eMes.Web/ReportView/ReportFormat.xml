      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText><%SQLQuery%></CommandText>
        <DataSourceName>MESRPT</DataSourceName>
      </Query>
      <Fields>
<%FieldList%>
      </Fields>
    </DataSet>
  </DataSets>
]]>
