<?xml version="1.0" encoding="utf-8" ?>
<data>
	<datatype name="WORKPLAN" text="生产计划">
		<field>
			<add key="PLANDATE" value="计划日期" Match="false" AllowNull="false"></add>
			<add key="BIGSSCODE" value="大线代码" Match="false" AllowNull="false"></add>
			<add key="PLANSEQ" value="生产顺序" Match="false" AllowNull="false" ></add>
			<add key="MOCODE" value="工单代码" Match="false" AllowNull="false"></add>
			<add key="MOSEQ" value="工单项次" Match="false" AllowNull="false"></add>
			<add key="PLANQTY" value="计划数量" Match="false" AllowNull="false"></add>
			<add key="PLANSTARTTIME" value="计划开始时间" Match="false" AllowNull="false"></add>
		</field>
	</datatype>

	<datatype name="TESTDATA" text="测试数据">
		<field>
			<add key="RCARD" value="产品序列号" Match="false" AllowNull="false"></add>
			<add key="MOCODE" value="工单代码" Match="false" AllowNull="false"></add>
			<add key="ITEMCODE" value="产品代码" Match="false" AllowNull="false" ></add>
			<add key="RESCODE" value="资源代码" Match="false" AllowNull="false" ></add>
			<add key="DEVICENO" value="设备编号" Match="false" AllowNull="false"></add>
			<add key="CKGROUP" value="检验项目组" Match="false" AllowNull="false"></add>
			<add key="CKITEMCODE" value="检验项目代码" Match="false" AllowNull="false"></add>
			<add key="PARAM" value="参数" Match="false" AllowNull="true"></add>
			<add key="USL" value="额定最大值" Match="false" AllowNull="true"></add>
			<add key="LSL" value="额定最小值" Match="false" AllowNull="true"></add>
			<add key="TESTINGVALUE" value="实际测试值" Match="false" AllowNull="false"></add>
			<add key="TESTINGRESULT" value="测试结果" Match="false" AllowNull="false"></add>
			<add key="TESTINGDATE" value="测试日期" Match="false" AllowNull="false"></add>
			<add key="TESTINGTIME" value="测试时间" Match="false" AllowNull="false"></add>
		</field>
	</datatype>

	<datatype name="MATERIALNEED" text="要料维护">
		<field>
			<add key="ITEMCODE" value="物料代码" Match="false" AllowNull="false"></add>
			<add key="ORGID" value="组织代码" Match="false" AllowNull="false" ></add>
			<add key="REQUESTQTY" value="需求标准数量" Match="false" AllowNull="true" ></add>
		</field>
	</datatype>

</data>
