  <DNTransferConfig>
    <WebService Url="http://localhost/test.asmx"  UserName="mesuser" Password="123456"></WebService>    
  </DNTransferConfig>

  <DNConfirmConfig>
    <WebService Url="http://localhost/test.asmx"  UserName="mesuser" Password="123456"></WebService>
  </DNConfirmConfig>

  <InventoryTransferConfig>
    <WebService Url="http://localhost/test.asmx"  UserName="mesuser" Password="123456"></WebService>
  </InventoryTransferConfig>

  <MaterialPOTransferConfig>
    <WebService Url="http://localhost/test.asmx"  UserName="mesuser" Password="123456"></WebService>
  </MaterialPOTransferConfig>

  <MaterialIssueTransferConfig>
    <WebService Url="http://localhost/test.asmx"  UserName="mesuser" Password="123456"></WebService>
